<robot name = "robot">
    <link name="TorsoFront">
        <inertial>
            <origin xyz="3 0 3.5" rpy="0 0 0"/>
            <mass value="1" />
            <inertia ixx="100" ixy="0" ixz="0" iyy="100" iyz="0" izz="100" />
        </inertial>
        <visual>
            <origin xyz="3 0 3.5" rpy="0 0 0"/>
            <geometry>
                <box size="2 1 1" />
            </geometry>
            <material name="Cyan">
                <color rgba="0 1.0 1.0 1.0"/>
            </material>
        </visual>
        <collision>
            <origin xyz="3 0 3.5" rpy="0 0 0"/>
            <geometry>
                <box size="2 1 1" />
            </geometry>
        </collision>
    </link>
    <joint name="TorsoFront_TorsoBack" type="revolute">
       <parent link="TorsoFront"/>
       <child  link="TorsoBack"/>
       <origin rpy="0 0 0" xyz="2 0 3.5" />
     <axis xyz="0 1 0"/>
       <limit effort="0.0" lower="-3.14159" upper="3.14159" velocity="0.0"/>
    </joint>
